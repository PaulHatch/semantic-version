{
  "parser": "@typescript-eslint/parser",
  "plugins": ["jest"],
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
